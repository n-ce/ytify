# ytify Backend Environment Variables

# Server
PORT=3000

# CORS allowed origins (comma-separated)
ALLOWED_ORIGINS=https://music.ml4-lab.com,http://localhost:5173

# Database path
DB_PATH=./data/ytify.db

# RapidAPI keys for YouTube fallback (comma-separated)
# Get keys from: https://rapidapi.com/ytjar/api/yt-api
RAPIDAPI_KEYS=key1,key2,key3

# Email validator URL
EMAIL_VALIDATOR_URL=https://rapid-email-verifier.fly.dev/api/validate

# Cache settings
CACHE_MAX_SIZE=500
CACHE_TTL_MS=300000

# Rate limiting
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=100
