{
  "name": "ytify",
  "version": "8.2.1-ml4.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:prod": "cross-env YTIFY_ENV=production tsc && cross-env YTIFY_ENV=production vite build --mode production",
    "build:dev": "cross-env YTIFY_ENV=development tsc && cross-env YTIFY_ENV=development vite build --mode development",
    "preview": "vite preview",
    "ship": "node scripts/ship.js",
    "sync": "node scripts/sync.js",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "solid-js": "1.9.10",
    "sortablejs": "^1.15.6"
  },
  "devDependencies": {
    "@netlify/blobs": "^10.5.0",
    "@netlify/edge-functions": "^3.0.3",
    "@netlify/functions": "^5.1.2",
    "@types/node": "^25.0.7",
    "@types/sortablejs": "^1.15.9",
    "@vercel/node": "^5.5.17",
    "autoprefixer": "^10.4.23",
    "cross-env": "^10.1.0",
    "eruda": "^3.4.3",
    "open-props": "1.7.17",
    "postcss-jit-props": "1.0.16",
    "terser": "^5.46.0",
    "typescript": "^5.9.3",
    "vite": "7.3.1",
    "vite-plugin-pwa": "^1.2.0",
    "vite-plugin-solid": "^2.11.10",
    "web-vitals": "^5.1.0",
    "workbox-background-sync": "^7.4.0",
    "workbox-cacheable-response": "^7.4.0",
    "workbox-expiration": "^7.4.0",
    "workbox-precaching": "^7.4.0",
    "workbox-range-requests": "^7.4.0",
    "workbox-routing": "^7.4.0",
    "workbox-strategies": "^7.4.0"
  },
  "browserslist": [
    "defaults"
  ]
}
